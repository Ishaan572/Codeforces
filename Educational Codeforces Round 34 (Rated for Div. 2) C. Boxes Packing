import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        
        Map<Integer, Integer> counts = new HashMap<>();
        int maxFreq = 0;
        
        for (int i = 0; i < n; i++) {
            int size = sc.nextInt();
            int currentCount = counts.getOrDefault(size, 0) + 1;
            counts.put(size, currentCount);
            
            // The answer is just the highest frequency we find
            maxFreq = Math.max(maxFreq, currentCount);
        }
        
        System.out.println(maxFreq);
    }
}
